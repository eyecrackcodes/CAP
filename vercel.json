{
  "builds": [
    {
      "src": "api_minimal.py",
      "use": "@vercel/python",
      "config": {
        "pip_requirements": "minimal-requirements.txt",
        "maxLambdaSize": "10mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api_minimal.py"
    },
    {
      "src": "/(.*)",
      "dest": "api_minimal.py"
    }
  ],
  "env": {
    "VERCEL": "true",
    "PYTHONUNBUFFERED": "1"
  }
}
