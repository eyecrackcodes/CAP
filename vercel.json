{
  "builds": [
    {
      "src": "api_only.py",
      "use": "@vercel/python",
      "config": {
        "pip_requirements": "requirements-vercel.txt",
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api_only.py"
    },
    {
      "src": "/(.*)",
      "dest": "api_only.py"
    }
  ],
  "env": {
    "VERCEL": "true",
    "PYTHONUNBUFFERED": "1"
  }
}
